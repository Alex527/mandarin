<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mpv="clr-namespace:Mandarin.Presentation.Views"
                    xmlns:mbc="clr-namespace:Mandarin.Business.Core;assembly=Mandarin.Business"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type system:DBNull}">
                <Separator />
            </DataTemplate>
            <DataTemplate DataType="{x:Type mbc:DockItem}">
                <mpv:DockItemView />
            </DataTemplate>
        </ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>