<Application x:Class="WinDock3.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wd="clr-namespace:WinDock3.Presentation.ViewModel">
    <Application.Resources>
        <DataTemplate x:Key="TextTemplate">
            <MenuItem Foreground="White" FontFamily="Lucida Sans Unicode" Header="{Binding Text}"/>
        </DataTemplate>
        <DataTemplate x:Key="SeparatorTemplate">
            <Separator />
        </DataTemplate>
    </Application.Resources>
</Application>
